<view style="width:100%;height:100%;background:#F9F5E6">
      <view class="detaiList" wx:for="{{recordList}}"  bindtap="getDetail" data-id="{{item.id}}"
                data-type="{{item.type}}"
                data-name="{{item.name}}">
        <image src="{{item.logoUrl}}" mode="widthFix" style="width:55px" class="coverImg"></image>
        <text class="subject">{{item.name}}</text>
        <image src="/images/gengduo.png" mode="widthFix" style="width:7px;" class="more"></image>
      </view>
</view>